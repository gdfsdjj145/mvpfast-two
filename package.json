{
  "name": "mvpfast",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "mcp": "tsx mcp/server.ts",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "fumadocs-mdx",
    "dev:all": "npm-run-all --parallel dev mcp",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "docker:build": "docker build -t mvpfast-web .",
    "docker:run": "docker run -p 3000:3000 --env-file .env mvpfast-web",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "docker:rebuild": "docker compose up -d --build"
